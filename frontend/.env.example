# BioMapper Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# API Endpoints
REACT_APP_API_URL=http://localhost:5000
REACT_APP_WEBSOCKET_URL=ws://localhost:5000

# Map Services
REACT_APP_MAPBOX_TOKEN=pk.eyJ1IjoiYmlvbWFwcGVyIiwiYSI6ImNrOXh5your_mapbox_token_here

# Authentication
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Payment Processing
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Feature Flags
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_QUANTUM_FEATURES=false
REACT_APP_ENABLE_BLOCKCHAIN_AUDIT=false
REACT_APP_DEBUG_MODE=false

# External Services
REACT_APP_PYTHON_ENGINE_URL=http://localhost:8001
REACT_APP_XAI_VISUALIZATION_URL=http://localhost:8003

# UI Configuration
REACT_APP_DEFAULT_MAP_CENTER_LAT=20.5937
REACT_APP_DEFAULT_MAP_CENTER_LNG=78.9629
REACT_APP_DEFAULT_MAP_ZOOM=5

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_TIMEOUT_MS=300000
